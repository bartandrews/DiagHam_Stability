@article {Spantoneaan8458,
	author = {Spanton, Eric M. and Zibrov, Alexander A. and Zhou, Haoxin and Taniguchi, Takashi and Watanabe, Kenji and Zaletel, Michael P. and Young, Andrea F.},
	title = {Observation of fractional Chern insulators in a van der Waals heterostructure},
	year = {2018},
	publisher = {American Association for the Advancement of Science},
	abstract = {Topologically ordered phases are characterized by long-range quantum entanglement and fractional statistics rather than by symmetry breaking. First observed in a fractionally filled continuum Landau level, topological order has since been proposed to arise more generally at fractional fillings of topologically non-trivial {\textquotedblleft}Chern{\textquotedblright} bands. Here, we report the observation of gapped states at fractional fillings of Harper-Hofstadter bands arising from the interplay of a magnetic field and a superlattice potential in a bilayer graphene/hexagonal boron nitride heterostructure. We observe phases at fractional filling of bands with Chern indices . Some of these, in and bands, are characterized by fractional Hall conductance{\textemdash}they are {\textquotedblleft}fractional Chern insulators{\textquotedblright} and constitute an example of topological order beyond Landau levels.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/early/2018/02/28/science.aan8458.full},
	journal = {Science}
}

