@article{jackson_geometric_2015,
  title = {Geometric stability of topological lattice phases},
  volume = {6},
  copyright = {2015 Nature Publishing Group},
  issn = {2041-1723},
  url = {https://www.nature.com/articles/ncomms9629},
  doi = {10.1038/ncomms9629},
  abstract = {The fractional quantum Hall (FQH) effect illustrates the range of novel phenomena which can arise in a topologically ordered state in the presence of strong interactions. The possibility of realizing FQH-like phases in models with strong lattice effects has attracted intense interest as a more experimentally accessible venue for FQH phenomena which calls for more theoretical attention. Here we investigate the physical relevance of previously derived geometric conditions which quantify deviations from the Landau level physics of the FQHE. We conduct extensive numerical many-body simulations on several lattice models, obtaining new theoretical results in the process, and find remarkable correlation between these conditions and the many-body gap. These results indicate which physical factors are most relevant for the stability of FQH-like phases, a paradigm we refer to as the geometric stability hypothesis, and provide easily implementable guidelines for obtaining robust FQH-like phases in numerical or real-world experiments.},
  language = {en},
  urldate = {2018-04-23},
  journal = {Nature Communications},
  author = {Jackson, T. S. and Möller, Gunnar and Roy, Rahul},
  month = nov,
  year = {2015},
  pages = {8629},
  file = {Full Text PDF:files/1762/Jackson et al. - 2015 - Geometric stability of topological lattice phases.pdf:application/pdf;Snapshot:files/1763/ncomms9629.html:text/html}
}

